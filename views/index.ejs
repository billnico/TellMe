<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="link-wrap">
         <button id="clicka">click</button>
    </div>
    <script src="https://cdn.socket.io/4.6.0/socket.io.min.js" integrity="sha384-c79GN5VsunZvi+Q/WObgk2in0CbZsHnjEqvFxC5DxHn9lTfNce2WW6h2pH6u/kF+" crossorigin="anonymous"></script>
    <script>
        
        const socket= io();
        socket.emit("active","nicolas");
        socket.emit("check",{name:"nicolas"});
        socket.on("connection",()=>{
           console.log("connected");
        });

        let button=document.getElementById("clicka");
        button.addEventListener("click",(e)=>{
            socket.emit("message",{name:"nicolas",receiver:"james",message:"gee you are quite af"})
        })
    </script>
  </body>
</html>
